    (VALUE1, VALUE2, ...),
    (VALUE1, VALUE2, ...),
    ...;

#PRIMARY KEY
CREATE TABLE table_name (
    column_name DATATYPE PRIMARY KEY,
    other_column DATATYPE
);

#FOREIGNKEY 
CREATE TABLE ChildTable (
    Column1 DATATYPE,
    ForeignKeyColumn DATATYPE REFERENCES ParentTable(ParentPrimaryKey)
